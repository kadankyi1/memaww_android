      android:visibility="visible"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent">

    <TextView
        android:id="@+id/activity_buysubscription_terms_textview"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:textAlignment="textStart"
        android:visibility="gone"
        android:text="By subscribing, you agree to our terms and fair use policy. Click to read."
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/activity_buysubscription_proceedholder_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/activity_buysubscription_proceedholder_button"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/button_rounded_green_pressed_white"
        android:text="Buy"
        android:layout_marginBottom="15dp"
        android:visibility="gone"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@+id/activity_buysubscription_terms_textview"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

  </androidx.constraintlayout.widget.ConstraintLayout>


  <FrameLayout
      android:id="@+id/activity_buysubscription_formholder_fragment"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:elevation="10dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/activity_buysubscription_v_guideline7"
      app:layout_constraintVertical_bias="1.0" />

  <androidx.core.widget.ContentLoadingProgressBar
      android:id="@+id/activity_buysubscription_loader"
      style="@style/ProgressBarBlue"
      android:layout_width="26dp"
      android:layout_height="32dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:layout_marginEnd="8dp"
      android:visibility="visible"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />


  <ImageView
      android:id="@+id/activity_buysubscription_reload_imageview"
      style="@style/ProgressBarBlue"
      android:layout_width="100dp"
      android:layout_height="100dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:layout_marginEnd="8dp"
      android:visibility="gone"
      android:src="@drawable/ic_baseline_refresh_24"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_chainStyle="packed"
      app:tint="@color/black" />


  <TextView
      android:id="@+id/activity_buysubscription_info_textview"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="There are no subscription offers"
      android:textColor="@color/color_gray"
      android:visibility="gone"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/activity_buysubscription_reload_imageview"
      app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>