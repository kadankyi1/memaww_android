      app:layout_constraintEnd_toStartOf="@+id/activity_buysubscription_v_guideline2"
      app:layout_constraintStart_toEndOf="@+id/activity_buysubscription_v_guideline1"
      app:layout_constraintTop_toBottomOf="@+id/activity_buysubscription_menuholder_constraintlayout"
      app:layout_constraintVertical_bias="0.0">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="10dp">

      <androidx.constraintlayout.widget.Guideline
          android:id="@+id/activity_buysubscription_formitemsholderscrollview_v_guideline3"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:orientation="vertical"
          app:layout_constraintGuide_begin="21dp" />


      <androidx.constraintlayout.widget.ConstraintLayout
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          >
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/activity_buysubscription_guideline_h1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.05" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/activity_buysubscription_guideline_v1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.05" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/activity_buysubscription_guideline_v2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.95" />

