        app:layout_constraintEnd_toStartOf="@+id/activity_buysubscription_menuholder_v_guideline6"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <TextView
        android:id="@+id/activity_buysubscription_menuholder_title_textview"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Buy Subscription"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:gravity="center"
        app:layout_constraintStart_toEndOf="@+id/activity_buysubscription_menuholder_v_guideline6"
        app:layout_constraintEnd_toStartOf="@+id/activity_buysubscription_menuholder_v_guideline7"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
