              app:layout_constraintTop_toBottomOf="@+id/activity_buysubscription_timeinfo_textview" />

        </androidx.constraintlayout.widget.ConstraintLayout>


        <ImageView
            android:id="@+id/activity_buysubscription_pickuplocation_roundedcornerimageview"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:scaleType="centerCrop"
            android:src="@drawable/map"
            app:layout_constraintBottom_toBottomOf="@+id/activity_buysubscription_pickuplocation_edittext_layout_holder"
            app:layout_constraintEnd_toStartOf="@+id/activity_buysubscription_guideline_v2"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/activity_buysubscription_pickuplocation_edittext_layout_holder"
            app:layout_constraintTop_toBottomOf="@+id/activity_buysubscription_pickuplocdatetime_layout_holder" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/activity_buysubscription_pickuplocation_edittext_layout_holder"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:theme="@style/fragmentFormHintStyle"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            app:layout_constraintEnd_toStartOf="@+id/activity_buysubscription_pickuplocation_roundedcornerimageview"
            app:layout_constraintStart_toStartOf="@+id/activity_buysubscription_guideline_v1"
            app:layout_constraintTop_toBottomOf="@+id/activity_buysubscription_pickuplocdatetime_layout_holder">

