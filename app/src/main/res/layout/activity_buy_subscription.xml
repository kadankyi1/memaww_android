              app:layout_constraintEnd_toStartOf="@+id/activity_buysubscription_timeinfo_12months_textview"
              app:layout_constraintStart_toEndOf="@+id/activity_buysubscription_timeinfo_3months_textview"
              app:layout_constraintTop_toBottomOf="@+id/activity_buysubscription_monthsinfo_textview" />


          <TextView
              android:id="@+id/activity_buysubscription_timeinfo_12months_textview"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginEnd="5dp"
              android:layout_marginStart="10dp"
              android:layout_marginTop="10dp"
              android:background="@drawable/button_white_background_pressed_gray"
