                        app:layout_constraintStart_toEndOf="@+id/activity_book_justwashquantity_textview"
                        app:layout_constraintTop_toBottomOf="@+id/activity_book_justwashquantity_textview"
                        app:layout_constraintVertical_bias="0.59000003" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:id="@+id/activity_book_formitemsholderscrollview_washandiron_constraintlayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="5dp"
                android:elevation="10dp"
                android:visibility="gone"
                app:cardCornerRadius="15dp"
                app:layout_constraintBottom_toTopOf="@+id/activity_book_formitemsholderscrollview_justwashnonwearables_constraintlayout"
