                        android:textColor="@color/color_gray"
                        android:textSize="16sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/activity_book_dateicon_imageview"
                        app:layout_constraintTop_toBottomOf="@+id/activity_book_formitemsholderscrollview_pickupcollectionlabel_textview" />


                    <ImageView
