                app:layout_constraintEnd_toStartOf="@+id/activity_book_formitemsholderscrollview_v_guideline4"
                app:layout_constraintStart_toEndOf="@+id/activity_book_formitemsholderscrollview_v_guideline3">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/activity_book_formitemsholderscrollview_washandironiconholder_constraintlayout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
