                        android:layout_marginStart="20dp"
                        android:layout_marginTop="10dp"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/activity_book_formitemsholderscrollview_pickupiconholder_constraintlayout"
                        app:layout_constraintTop_toBottomOf="@+id/activity_book_formitemsholderscrollview_pickupcollectionlabel_textview" />

                    <TextView
                        android:id="@+id/activity_book_date_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
