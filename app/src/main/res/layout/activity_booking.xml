                    </androidx.cardview.widget.CardView>


                    <TextView
                        android:id="@+id/activity_book_formitemsholderscrollview_justwashcollectionlabel_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginStart="20dp"
                        android:text="Lightweight Items"
                        android:textColor="@color/black"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/activity_book_formitemsholderscrollview_justwashiconholder_constraintlayout"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/activity_book_justwashdate_textview"
                        android:layout_width="wrap_content"
