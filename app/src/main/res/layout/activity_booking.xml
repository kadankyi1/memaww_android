                        app:layout_constraintTop_toBottomOf="@+id/activity_book_time_textview"
                        app:layout_constraintVertical_bias="0.59000003" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
