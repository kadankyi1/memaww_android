                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/activity_book_formitemsholderscrollview_justwashiconholder_constraintlayout"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/activity_book_formitemsholderscrollview_justwashcollectionlabel_textview" />


                    <TextView
                        android:id="@+id/activity_book_justwashquantity_textview"
                        android:layout_width="wrap_content"
