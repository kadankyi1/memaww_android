                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/activity_book_formitemsholderscrollview_deliveryiconholder_constraintlayout"
                        app:layout_constraintTop_toTopOf="parent" />


                    <ImageView
