                        android:layout_marginStart="20dp"
                        android:text="10 items"
                        android:textColor="@color/black"
                        android:textSize="16sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/activity_book_formitemsholderscrollview_justwashiconholder_constraintlayout"
                        app:layout_constraintTop_toBottomOf="@+id/activity_book_justwashdate_textview"
